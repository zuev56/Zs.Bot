using System;
using System.Collections.Generic;
using System.Text;
using Zs.Bot.Model.Db;

namespace Zs.Bot
{
    public static class MessageFactory
    {
        public static DbReceivedMessage NewReceivedMessage()
        {
            return new DbReceivedMessage
            {
                InsertDate = DateTime.Now,
                UpdateDate = DateTime.Now
            };
        }

        public static DbSentMessage NewSentMessage()
        {
            return new DbSentMessage
            {
                InsertDate = DateTime.Now,
                UpdateDate = DateTime.Now
            };
        }
    }
}
